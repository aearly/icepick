{
  "preset": "google",
  "validateQuoteMarks": "\"",
  "validateJSDoc": {
    "requireParamTypes": false
  },
  "disallowMultipleLineBreaks": null,
  "disallowMultipleVarDecl": null,
  "requireLineFeedAtFileEnd": null,
  "maximumLineLength": {
    "value": 90,
    "allowComments": false,
    "allowRegex": true
  },
  "disallowSpacesInAnonymousFunctionExpression": null,
  "requireSpaceAfterKeywords": [
    "if",
    "else",
    "for",
    "while",
    "do",
    "switch",
    "return",
    "try",
    "catch",
    "function"
  ]
}
